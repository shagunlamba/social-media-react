nst localStrUser = JSON.parse(localStorage.getItem('user-info'));\n        setUser(localStrUser.username);\n\n        axios.get(`${BASE_URL}/upload/userPosts`, {\n            params: {\n                username: username\n            }\n        })\n        .then((res)=>{\n            console.log(res);\n            setPostsArray(res.data);\n        })\n        .catch((err)=>{\n            console.log(err);\n        })\n\n        axios.get(`${BASE_URL}/upload/userDetails`,{\n            params: {\n                username: username\n            }\n        })\n        .then((res)=>{\n            console.log(\"The res\", res);\n            setUserDetails(()=>{\n                return {\n                    username: res.data[0].username,\n                    fullName: res.data[0].fullName,\n                    age: res.data[0].age,\n                    gender: res.data[0].gender\n                }\n            })\n        })\n        .catch((err)=>{\n            console.log(err);\n        })\n\n    }, [show]);\n\n\n   \n\n    const deletePost = (postID) => {\n\n        console.log(\"id\", postID);\n        axios.post(`${BASE_URL}/upload/deletePost`, { postID : postID })\n        .then((res)=>{\n            console.log(res);\n            console.log(\"Deleted!\");\n        })\n        .catch((err)=>{\n            console.log(err);\n        })\n\n        setShow(false);\n    }\n\n    const logoutHandler= ()=>{\n        swal({\n            title: \"Logged out!\",\n            icon: \"success\",\n        });\n        localStorage.removeItem('user-info');\n        history.push('/');\n    }\n\n    \n    return (\n        <div>\n            <UserNavbar iconName=\"profile\" />\n            <div>\n                <header>\n                    <Row className=\"userArea\">\n                        <Col xs={12} className=\"user\">\n                            <div style={{display: \"flex\", justifyContent: \"space-between\"}}>\n                                <div>\n               